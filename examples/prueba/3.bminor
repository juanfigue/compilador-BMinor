/* Suma de elementos de un array */
SIZE: integer = 10;
numbers: array [SIZE] integer;

main: function void () = {
    i: integer;
    sum: integer = 0;

    for (i = 0; i < SIZE; i++) {
        numbers[i] = i * 2;
    }

    for (i = 0; i < SIZE; i++) {
        sum = sum + numbers[i];
    }

    print "Suma total: ", sum, "\n";
}