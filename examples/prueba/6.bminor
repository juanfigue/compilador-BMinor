/* NÃºmeros perfectos menores que 500 */
main: function void () = {
    n: integer;
    i: integer;
    sum: integer;

    for (n = 1; n < 500; n++) { 
        sum = 0;
        for (i = 1; i < n; i++) {
            if (n % i == 0) {
                sum = sum + i;
            }
        }
        if (sum == n) {
            print n, "\n";
        }
    }
}